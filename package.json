		"eslint-plugin-svelte3": "3.4.1",
		"prettier": "2.5.1",
		"prettier-plugin-svelte": "2.6.0",
		"svelte": "3.46.4",
		"svelte-check": "2.4.5",
		"svelte-preprocess": "4.10.4",
		"tslib": "2.3.1",
		"typescript": "4.6.2"
	},
	"dependencies": {
		"@sveltejs/adapter-static": "1.0.0-next.28",
